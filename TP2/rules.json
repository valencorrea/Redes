{
    "firewall_switch_id" : '00-00-00-00-00-01',
    "rules": [
      {
        "description": "Block UDP port 80",
        "rule": {
          "dst_port": 80
        }
      },
      {
        "description": "Block UDP port 5001 and host 1",
        "rule": {
          "protocol": "udp",
          "dst_port": 5001,
          "dl_src": "00:00:00:00:01"
        }
      },
      {
        "description": "Block communication between host 1 and host 4",
        "rule": {
          "dl_src": "00:00:00:00:01",
          "dl_dst": "00:00:00:00:04"
        }
      }
    ]
}